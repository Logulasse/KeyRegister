@model KeyRegister.ViewModels.ReservationVM
@{
    ViewBag.Title = "Reservation";
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">@ViewBag.Title</h1>
    </div>

    <div class="col-md-4">
        @using (Html.BeginForm())
        {
            @Html.ValidationMessageFor(model => model.Nom)
            @Html.ValidationMessageFor(model => model.Prenom)
            @Html.ValidationMessageFor(model => model.IdBooster)
            @Html.ValidationMessageFor(model => model.DateDeb)
            @Html.ValidationMessageFor(model => model.DateFin)
            @Html.EditorForModel()
            <br />
            <input type="submit" value="Valider ma Réservation" />
        }
    </div>
</div>